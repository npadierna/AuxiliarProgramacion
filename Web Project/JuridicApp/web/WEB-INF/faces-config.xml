<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd" >

    <application>
        <locale-config>
            <default-locale>en_US</default-locale>
        </locale-config>
    </application>
    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
    <managed-bean>
        <managed-bean-name>creatorEntityBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.admin.creator.CreatorEntityBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>loginUserBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.user.login.LoginUserBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>registrationUserBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.user.registration.RegistrationUserBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>updateUserBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.user.update.UpdateUserBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>workListBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.work.WorkListBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>workSelectorBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.work.WorkSelectorBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>creatorWorkBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.work.create.CreatorWorkBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>dependencyBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.dependency.DependencyBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>profileBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.profile.ProfileBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>roleBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.role.RoleBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>supportBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.support.SupportBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>typeBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.type.TypeBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <!-- Tester -->
    <managed-bean>
        <managed-bean-name>authorFinderTesterBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.juridicapp.web.bean.tester.AuthorFinderTesterBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>